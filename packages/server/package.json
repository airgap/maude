{
  "name": "@e/server",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "bun run --hot src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "build:binary": "bun build src/index.ts --compile --outfile ../../src-tauri/binaries/e-server",
    "build:standalone": "bun build src/standalone.ts --compile --outfile ../../dist/standalone/e",
    "check": "tsc --noEmit",
    "test": "bun test",
    "test:coverage": "bun test --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.991.0",
    "@e/shared": "workspace:*",
    "editorconfig": "^3.0.1",
    "hono": "^4.7.0",
    "nanoid": "^5.0.0",
    "node-pty": "^1.1.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.6.0"
  }
}
